\newbibmacro*{name:corp:both}[2]{%%
  \usebibmacro{name:delim}{#1}%%
  \usebibmacro{name:hook}{#1}%%
  \mkbibnamecorplong{#1}%%
  \ifdefvoid{#2}{}{\space\mkbibnamecorpshortintro{#2}}}%
